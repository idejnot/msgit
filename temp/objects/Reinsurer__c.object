    <validationRules>
        <fullName>Accepted_Reinsurers_cannot_be_edited</fullName>
        <active>true</active>
        <description>Accepted placement cannot be edited</description>
        <errorConditionFormula>ISPICKVAL(Placement_Status__c, &apos;Accepted&apos;)</errorConditionFormula>
        <errorDisplayField>Placement_Status__c</errorDisplayField>
        <errorMessage>Cannot edit an accepted reinsurer placement</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Authorized_share_must_be_LE_100</fullName>
        <active>true</active>
        <errorConditionFormula>Not(Authorized__c   &lt;= 100)</errorConditionFormula>
        <errorDisplayField>Authorized__c</errorDisplayField>
        <errorMessage>Authorized % must be less than or equal to 100%</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Signed_must_be_LTE_authorized</fullName>
        <active>true</active>
        <description>Signed must be &lt;= Authorized amount</description>
        <errorConditionFormula>Signed__c  &gt;  Authorized__c</errorConditionFormula>
        <errorDisplayField>Signed__c</errorDisplayField>
        <errorMessage>Signed share must be &lt;= Authorized share</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Reinsurer_Type</fullName>
        <active>false</active>
        <description>ensure accounts selected are either reinsurers or syndiacate</description>
        <errorConditionFormula>Not(ISPICKVAL(Reinsurer__r.Type, &quot;Reinsurer&quot;))</errorConditionFormula>
        <errorDisplayField>Reinsurer__c</errorDisplayField>
        <errorMessage>Placing entity must be Reinsurer</errorMessage>
    </validationRules>
